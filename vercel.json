{
  "version": 2,
  "builds": [
    {
      "src": "api/socket.js",
      "use": "@vercel/node",
      "config": {
        "supportsResponseStreaming": true
      }
    }
  ],
  "routes": [
    {
      "src": "/socket.io/(.*)",
      "dest": "api/socket.js"
    }
  ],
  "functions": {
    "api/socket.js": {
      "maxDuration": 30
    }
  }
}
